{
    "name": "ts_next_app",
    "version": "0.1.3",
    "private": true,
    "scripts": {
        "dev": "next dev -p 8000",
        "dev:workspace": "zellij --layout .zellij-layout.kdl",
        "type-check": "tsc --noEmit --project tsconfig.json",
        "type-check:watch": "tsc --noEmit --watch --preserveWatchOutput --project tsconfig.json",
        "validate": "npm run type-check && npm run lint",
        "prebuild": "npm run type-check && npm run lint && node scripts/generate_build_info.js && mkdir -p public/onnx && cp ../../node_modules/onnxruntime-web/dist/*.wasm public/onnx/ && cp ../../node_modules/onnxruntime-web/dist/*.mjs public/onnx/ && npm run cleanup-onnx",
        "cleanup-onnx": "rm -f public/onnx/*.js public/onnx/*.map public/onnx/ort.all.bundle.* public/onnx/ort.all.mjs public/onnx/ort.bundle.* public/onnx/ort.mjs public/onnx/ort.min.mjs public/onnx/ort.webgl.* public/onnx/ort.webgpu.* public/onnx/ort.node.* public/onnx/*asyncify* public/onnx/*jsep* public/onnx/silero_vad_legacy.onnx && rm -rf public/onnx/cpu",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "test": "NODE_ENV=test npx jest --runInBand --silent",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:headed": "playwright test --headed",
        "test:e2e:debug": "playwright test --debug",
        "test:e2e:report": "playwright show-report"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.71.2",
        "@chakra-ui/icons": "^2.2.4",
        "@chakra-ui/react": "^2.10.6",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@fontsource/roboto": "^5.0.13",
        "@google/genai": "^1.34.0",
        "@modelcontextprotocol/sdk": "^1.22.0",
        "@mui/icons-material": "^6.4.7",
        "@mui/material": "^6.4.7",
        "@mui/system": "^6.4.7",
        "@openai/agents": "^0.2.1",
        "@react-sigma/core": "^5.0.4",
        "@react-sigma/layout-forceatlas2": "^5.0.4",
        "@ricky0123/vad-react": "^0.0.36",
        "@sigma/edge-curve": "^3.1.0",
        "ace-builds": "^1.37.1",
        "crypto": "^1.0.1",
        "eslint": "^8.57.0",
        "firebase": "^10.12.4",
        "graphology": "^0.26.0",
        "graphology-layout": "^0.6.1",
        "graphology-layout-forceatlas2": "^0.10.1",
        "graphology-types": "^0.24.8",
        "gray-matter": "^4.0.3",
        "highlight.js": "^11.11.1",
        "next": "^14.2.3",
        "onnxruntime-web": "^1.23.2",
        "react": "^18",
        "react-ace": "^13.0.0",
        "react-dom": "^18",
        "react-firebase-hooks": "^5.1.1",
        "react-grid-layout": "^2.2.2",
        "react-markdown": "^9.0.1",
        "rehype-highlight": "^7.0.2",
        "remark-gfm": "^4.0.1",
        "sigma": "^3.0.2",
        "three": "^0.160.0",
        "tidyscripts_node": "*",
        "tidyscripts_web": "*",
        "zod": "^3.25.76",
        "zustand": "^5.0.9"
    },
    "devDependencies": {
        "@playwright/test": "^1.57.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "@types/react-grid-layout": "^1.3.6",
        "@types/three": "^0.182.0",
        "copy-webpack-plugin": "^13.0.1",
        "eslint-config-next": "14.2.2",
        "jest": "^30.0.2",
        "jest-environment-jsdom": "^30.0.2",
        "jest-html-reporters": "^3.1.7",
        "node-polyfill-webpack-plugin": "^4.1.0",
        "typescript": "^5"
    }
}
