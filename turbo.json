{
    "$schema": "https://turborepo.org/schema.json",
    "tasks": {
	"dev": {
	    "cache": false
	},
	"build": {
	    "dependsOn": [ "^build" ],
	    "outputs": [
		".next/**",
		"dist/**",
		"functions/lib/**",
		"apps/ts_next_app/public/docs/**",
		"apps/ts_next_app/generated/**"	  		
	    ]
	},
	"ts_next_app#build": {
	    "dependsOn": [ "docs#build" ],
	    "outputs": [
		".next/**"
	    ],
	    "inputs": ["$TURBO_DEFAULT$", "!generated/**"]
	},
	"docs#build": {
	    "dependsOn": [ 
		"tidyscripts_common#build",
		"tidyscripts_node#build", 
		"tidyscripts_web#build",
		"tidyscripts_web_umd#build"
	    ],
	    "outputs": [
		"../ts_next_app/public/docs/**"
	    ]
	},
	"publish_package": {
	    "dependsOn": [
		"build",
		"patch_package",
		"^publish_package"
	    ],
	    "outputs": []
	},
	"patch_package": {
	    "dependsOn": [
		"^patch_package"
	    ],
	    "outputs": []
	},
	"docs": {
	    "outputs": [
		"apps/ts_next_app/public/docs"
	    ]
	}
    }
}
