{
    "$schema": "https://turborepo.org/schema.json",
    "tasks": {
	"dev": {
	    "cache": false
	},
	"prebuild": {
	    "outputs": ["generated/**"]
	},
	"build": {
	    "dependsOn": [ "prebuild" , 
			   "^build"
	    ],
	    "outputs": [
		".next/**",
		"dist/**",
		"functions/lib/**",
		"apps/ts_next_app/public/docs/**"	  
	    ]
	},
	"publish_package": {
	    "dependsOn": [
		"build",
		"patch_package",
		"^publish_package"
	    ],
	    "outputs": []
	},
	"patch_package": {
	    "dependsOn": [
		"^patch_package"
	    ],
	    "outputs": []
	},
	"docs": {
	    "outputs": [
		"apps/ts_next_app/public/docs"
	    ]
	}
    }
}
