
import * as cu from "../common/utils" 
import * as http from "./http" 

// assign the json retriever
cu.set_json_retriever( http.get_json ) ;



// asign  the image retriever
let img_retriever = async function (url:string) {
    let res  = await http.fetch(url, {method: 'POST'}) 
    return await res.blob()
} 
cu.set_image_retriever( img_retriever )


